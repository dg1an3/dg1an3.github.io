#################################################################
#    Object Properties
#################################################################

:has_character rdf:type owl:ObjectProperty .
:made rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Classes
#################################################################

:Agent rdf:type owl:Class ;
       rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000002> .
:Group rdf:type owl:Class ;
       rdfs:subClassOf :Agent , <http://purl.obolibrary.org/obo/BFO_0000027> .
:Organization rdf:type owl:Class ;
              rdfs:subClassOf :Agent ,
                              <http://purl.obolibrary.org/obo/BFO_0000027> .
:Person rdf:type owl:Class ;
        rdfs:subClassOf :Agent ,
                        :Animal .

:Author rdf:type owl:Class ;
        rdfs:subClassOf [ owl:intersectionOf ( :Person
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <http://purl.obolibrary.org/obo/RO_0000087> ;
                                                 owl:someValuesFrom <http://purl.obolibrary.org/obo/IAO_0000442>
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] .
:Chapter rdf:type owl:Class ;
         rdfs:subClassOf <http://purl.obolibrary.org/obo/IAO_0000314> .
:FictionalCharacter rdf:type owl:Class ;
    rdfs:subClassOf :Agent , <http://purl.obolibrary.org/obo/BFO_0000031> .

:Goal rdf:type owl:Class ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/IAO_0000005> .
:FoodStuff rdf:type owl:Class ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000040> .

:ManufacturedObject rdf:type owl:Class ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000030> .

:FictionalWork rdf:type owl:Class ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/IAO_0000310> .
:Novel rdf:type owl:Class ;
       rdfs:subClassOf :FictionalWork .
:Play rdf:type owl:Class ;
      rdfs:subClassOf :FictionalWork .

:Organism rdf:type owl:Class ;
          rdfs:subClassOf <http://purl.obolibrary.org/obo/BFO_0000030> .
:Plant rdf:type owl:Class ;
       rdfs:subClassOf :Organism .
:Animal rdf:type owl:Class ;
        rdfs:subClassOf :Organism .


